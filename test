            'ad_storage': consentDefaultValue,
            'ad_user_data': consentDefaultValue,
            'ad_personalization': consentDefaultValue,
            'analytics_storage': consentDefaultValue
        });
        
        if (hasGoogleIdFromUrl()) {
            useExternalGtmCode = 1;
        }
    </script>
